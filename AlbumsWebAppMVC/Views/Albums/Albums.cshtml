
@{
    ViewBag.Title = "Álbumes";
}

<script src="~/Scripts/javascript/albums.js" type="text/javascript"></script>

<h2>@ViewBag.Title</h2>

<table id="albumsTable" class="table table-striped table-hover">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Album name</th>
            <th scope="col">Description</th>
            <th scope="col">Creation Date</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">1</th>
            <th>Vacaciones en Cancún</th>
            <th>Vacaciones del 2018</th>
            <th>2018-10-10</th>
        </tr>
    </tbody>
</table>